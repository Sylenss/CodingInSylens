  "editor.tokenColorCustomizations": {
    "comments": "#4c556a",
    "variables": "#9e8b42",
    "keywords": "#d1b24a",
    "strings": "#9e8b42",
    "numbers": "#e7e68e",
    "functions": "#d99b21",
    "textMateRules": [
      {
        "name": "Settings String Name",
        "scope": [
          "support.type.property-name",
        ],
        "settings": {
          "foreground": "#7f723c"
        }
      },
      {
        "name": "Settings String bool",
        "scope": [
          "constant.language"
        ],
        "settings": {
          "foreground": "#aaab6f"
        }
      },
        // "Cpp Code Style"
      {
        "name": "Class Template",
        "scope": [
          "entity.name.type.class.templated"
        ],
        "settings": {
          "foreground": "#fec677"
        }
      },
      {
        "name": "User-Defined Literal - String",
        "scope": [
          "entity.name.operator.custom-literal.string",
          "keyword.other.typedef.c",
          "storage.modifier.c",
          "meta.preprocessor.macro.c"
        ],
        "settings": {
          "foreground": "#aaab6f"
        }
      },
      {
        "name": "Type",
        "scope": [
          "entity.name.type"
        ],
        "settings": {
          "foreground": "#9e8b42"
        }
      },
      {
        "name": "Global/Local Variables",
        "scope": [
          "variable.other.global",
          "variable.other.local"
        ],
        "settings": {
          "foreground": "#a77822"
        }
      },
      {
        "name": "Enumerator",
        "scope": [
          "variable.other.enummember",
          "entity.name.function.preprocessor",
          "punctuation.definition.parameters.begin.c",
          "punctuation.definition.parameters.end.c",
          "punctuation.section.parens.begin.bracket.round.c",
          "punctuation.section.parens.end.bracket.round.c"
        ],
        "settings": {
          "foreground": "#a77822"
        }
      },
      {
        "name": "SizeOf",
        "scope": [
          "keyword.operator.sizeof.c"
        ],
        "settings": {
          "foreground": "#ed8b27"
        }
      },
      {
        "name": "String Quoted",
        "scope": [
          "string.quoted.double.cpp"
        ],
        "settings": {
          "foreground": "#d99b21"
        }
      },
    ]
  },
  // other stuff
  "window.zoomLevel": 0,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "update.mode": "manual",
  "git.enabled": true,
  "git.decorations.enabled": true,
  "files.insertFinalNewline": true,
  "breadcrumbs.enabled": false,
  "explorer.autoReveal": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": false,
  "explorer.enableDragAndDrop": false,
  "editor.multiCursorModifier": "alt",
  // customizations
  "customizeUI.font.monospace": "Jetbrains Mono",
  "customizeUI.font.regular": "Jetbrains Mono",
  "customizeUI.fontSizeMap": {
    "13px": "13px",
    "monospace": "13px"
  },
  "customizeUI.stylesheet": {
    ".tabs-container .tab": "height: 18px !important",
    ".tabs": "height: 22px !important",
    "[aria-label='Editor actions']": "display: none !important;"
  }
