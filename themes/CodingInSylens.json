  "editor.tokenColorCustomizations": {
    "comments": "#4c556a",
    "variables": "#9e8b42",
    "keywords": "#d1b24a",
    "strings": "#9e8b42",
    "numbers": "#e7e68e",
    "functions": "#ed8b27",
    "textMateRules": [
      {
        "name": "Settings String Name",
        "scope": [
          "support.type.property-name",
          "variable.css"
        ],
        "settings": {
          "foreground": "#7f723c"
        }
      },
      //Php Code Scheme
      {
        "name": "Php Storage Modifers",
        "scope": [
          "storage.modifier",
          "entity.name.namespace"
        ],
        "settings": {
          "foreground": "#747ebe"
        }
      },
      {
        "name": "Php inherit Class Entity",
        "scope": [
          "entity.other.inherited-class",
        ],
        "settings": {
          "foreground": "#ee8b4e"
        }
      },
      {
        "name": "Php Storage Modifiers",
        "scope": [
          "storage.modifier.php",
        ],
        "settings": {
          "foreground": "#747EBE"
        }
      },
      {
        "name": "Types",
        "scope": [
          "keyword", //import, export, return…
          "constant", //String, Number, Boolean…, this, super
          "storage.modifier", //static keyword
          "storage.type.js", //types keyword
          "storage.type.class.js", //class keyword
          "storage.type.function.js", //function keyword
          "meta.definition.function.js", //function name
          "entity.other.attribute-name.html",
        ],
        "settings": {
          "foreground": "#9e8b42"
        }
      },
      {
        "name": "Settings String bool",
        "scope": [
          "constant.language"
        ],
        "settings": {
          "foreground": "#aaab6f"
        }
      },
      {
        "name": "Css Percentage",
        "scope": [
          "entity.other.keyframe-offset.percentage.css",
          "constant.language"
        ],
        "settings": {
          "foreground": "#e7e68e"
        }
      },
      // "Cpp Code Style"
      {
        "name": "Class Template",
        "scope": [
          "entity.name.type.class.templated"
        ],
        "settings": {
          "foreground": "#fec677"
        }
      },
      {
        "name": "User-Defined Literal - String",
        "scope": [
          "entity.name.operator.custom-literal.string",
          "keyword.other.typedef.c",
          "storage.modifier.c",
          "meta.preprocessor.macro.c"
        ],
        "settings": {
          "foreground": "#aaab6f"
        }
      },
      {
        "name": "Type",
        "scope": [
          "entity.name.type",
          "punctuation.section.property-list.begin.bracket.curly.css",
          "punctuation.section.property-list.end.bracket.curly.css",
          "punctuation.section.function.begin.bracket.round.css",
          "punctuation.section.function.end.bracket.round.css",
          "punctuation.terminator.rule.css",
          "punctuation.definition.block.js",
          "punctuation.terminator.statement.js",
          "punctuation.section.keyframes.begin.bracket.curly.css",
          "punctuation.section.keyframes.end.bracket.curly.css",
          "punctuation.definition.class.begin.bracket.curly.php",
          "punctuation.definition.class.end.bracket.curly.php",
          "punctuation.accessor.js"
        ],
        "settings": {
          "foreground": "#9e8b42"
        }
      },
      {
        "name": "Global/Local Variables",
        "scope": [
          "variable.other.global",
          "variable.other.local",
          "meta.embedded.block.php",
          "meta.function-call.php",
          "meta.function.php",
          "punctuation.separator.key-value",
          "entity.other.attribute-name"
        ],
        "settings": {
          "foreground": "#a77822"
        }
      },
      {
        "name": "Js Object Literal Keys",
        "scope": [
          "meta.object-literal.key"
        ],
        "settings": {
          "foreground": "#EE8B4E"
        }
      },
      {
        "name": "Js Variable Language",
        "scope": [
          "variable.language",
        ],
        "settings": {
          "foreground": "#7AA5FF"
        }
      },
      {
        "name": "Editor Config Color",
        "scope": [
          "keyword",
          "support.type",
          "keyword.other.using.directive.cpp"
        ],
        "settings": {
          "foreground": "#769AA5"
        }
      },
      {
        "name": "Enumerator",
        "scope": [
          "variable.other.enummember",
          "entity.name.function.preprocessor",
          //Css Punctaution Code
          "punctuation.definition.parameters.begin.c",
          "punctuation.definition.parameters.end.c",
          "punctuation.section.parens.begin.bracket.round.c",
          "punctuation.section.parens.end.bracket.round.c",
          //JS Punctuation
          "meta.brace.round.js",
          "punctuation.definition.parameters.begin.js",
          "punctuation.definition.parameters.end.js",
          "meta.brace.square.js",
          "punctuation.separator.parameter.js",
          "punctuation.separator.comma.js",
          "meta.property-value.css",
          "entity.other.attribute-name.id.css",
          "variable.other.constant.property",
          "keyword.operator.new.js",
        ],
        "settings": {
          "foreground": "#a77822"
        }
      },
      {
        "name": "Js Entity Name Class",
        "scope": [
          "entity.name.type.class",
          "variable.other.enummember"
        ],
        "settings": {
          "foreground": "#33736A"
        }
      },
      {
        "name": "SizeOf",
        "scope": [
          "keyword.operator.sizeof.c"
        ],
        "settings": {
          "foreground": "#ed8b27"
        }
      },
      {
        "name": "String Quoted",
        "scope": [
          "string.quoted.double.cpp"
        ],
        "settings": {
          "foreground": "#d99b21"
        }
      },
      {
        "name": "Variables and Constants",
        "scope": [
          "variable.other.readwrite.js",
          "variable.other.constant",
          "support.variable.js",
          "support.class",
          "entity.name.scope-resolution.cpp"
        ],
        "settings": {
          "foreground": "#4C7A96"
        }
      },
      {
        "name": "Js Property",
        "scope": [
          "variable.other.property.js",
          "variable.other.object.property.js",
          "support.variable.property.js",
          "support.variable.property"
        ],
        "settings": {
          "foreground": "#CB9826"
        }
      },
      {
        "name": "Js Function",
        "scope": [
          "support.function",
        ],
        "settings": {
          "foreground": "#5198A0"
        }
      },
      //HTML Code
      {
        "name": "Class Attribute",
        "scope": [
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "foreground": "#a77822"
        }
      },
      {
        "name": "Div Entity",
        "scope": [
          "entity.name.tag.html"
        ],
        "settings": {
          "foreground": "#d99b21"
        }
      },
      {
        "name": "Css Class Attribute",
        "scope": [
          "entity.other.attribute-name.class.css"
        ],
        "settings": {
          "foreground": "#a77822"
        }
      },
      {
        "name": "Flex Class Attribute",
        "scope": [
          "keyword.other.unit.px.css",
          "support.constant.property-value.css",
          "constant.other.color.rgb-value.hex.css",
          "keyword.other.unit.s.css",
          "entity.other.attribute-name.pseudo-class.css",
          "keyword.operator"
        ],
        "settings": {
          "foreground": "#D1B24A"
        }
      },
      {
        "name": "Rem Class Attribute",
        "scope": [
          "keyword.other.unit.rem.css",
        ],
        "settings": {
          "foreground": "#AAAB6F"
        }
      },
    ]
  },
  // other stuff
  "window.zoomLevel": 1,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "update.mode": "manual",
  "git.enabled": true,
  "git.decorations.enabled": true,
  "files.insertFinalNewline": true,
  "breadcrumbs.enabled": false,
  "explorer.autoReveal": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": false,
  "explorer.enableDragAndDrop": false,
  "editor.multiCursorModifier": "alt",
  // customizations
  "customizeUI.font.monospace": "Jetbrains Mono",
  "customizeUI.font.regular": "Jetbrains Mono",
  "customizeUI.fontSizeMap": {
    "13px": "16px",
    "monospace": "13px"
  },
  "customizeUI.stylesheet": {
    ".tabs-container .tab": "height: 24px !important; margin-top: -10px !important;",
    ".tabs": "height: 19px !important",
    ".tab-actions": "margin-top: 10px !important;",
    ".title .tabs": "height: 24px !important;",
    ".tabs-container": "height: 24px !important;",
    "[aria-label='Editor actions']": "display: none !important;",
    ".monaco-hover-content": "max-height: 450px !important;",
    ".monaco-editor-hover .monaco-editor-hover-content": "max-width: 1000px"
  }
}
